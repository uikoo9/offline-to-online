{
  "name": "offline-to-online",
  "version": "3.1.2",
  "description": "offline to online on nodejs and browser",
  "keywords": [
    "offline to online",
    "nodejs",
    "browser"
  ],
  "author": "uikoo9 <uikoo9@qq.com>",
  "homepage": "https://code.insistime.com/offline-to-online",
  "license": "MIT",
  "main": "index.js",
  "module": "src/index.js",
  "sideEffets": false,
  "files": [
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uikoo9/offline-to-online.git"
  },
  "bugs": {
    "url": "https://github.com/uikoo9/offline-to-online/issues"
  },
  "scripts": {
    "build": "rollup -c"
  },
  "nx": {
    "namedInputs": {
      "default": [
        "{projectRoot}/src/**/*"
      ]
    },
    "targets": {
      "build": {
        "inputs": [
          "default"
        ],
        "outputs": [
          "{projectRoot}/index.js"
        ]
      }
    }
  }
}
